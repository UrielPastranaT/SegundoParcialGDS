          <h2 class="centered">Crear Cuenta</h2>
          <div class="contacto-form php-mail-form">
            <form action="/admin/registrar" method="post" id="formulario">
              <div class="row">
                <div class="col-6">
                  <label for="lblUsuario">Usuario</label>
                  <input type="text" class="form-control" name="tfUsuario" required/>
                  <div class="validate"></div>
                </div>
                <div class="col-6">
                  <label for="lblPassword">Contraseña</label>
                  <input
                    type="password"
                    class="form-control"
                    name="tfPassword"
                    required
                  />
                  <div class="validate"></div>
                </div>
                <div class="form-send">
                  <button
                    type="submit"
                    class="btn btn-large"
                    name="btnIniciarSesion">
                    Registrarme
                  </button>
                </div>
              </div>
            </form>
